%type AStmt AStmt
%type Call Call
%type Dot Dot
%type Var Var 
%type EqPart EqPart

%token id 

%%

AStmt : EqPart '=' EqPart {$$=control:eq($1,$3)}
      | Call {$$=$1};

Dot : Var '.' Var {$$=control:dot($1,$3)};

Call : id '[' (id // ',')* ']' {$$=control:call($1,$3)} ;


Var : id {$$=$1} 
    | '?' id {$$=$2};

EqPart : Var {$$=$1}
       | Dot {$$=$1} 
       | Call{$$=$1};

%%
