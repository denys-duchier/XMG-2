
%%

Hierarchy : FType {$$=$1} | FConstraint {$$=$1}; 

FType : ftype id {$$=hierarchy:ftype($2)};

FConstraint : fconstraint IDsOrAttr FOp IDsOrAttr {$$=hierarchy:fconstraint($3,$2,$4)};

FOp : '->' {$$=$1} | '<->' {$$=$1} ;

IDsOrAttr : IDs {$$=ids($1)} | id '=' id {$$=pathEq($1,$3)} | id ':' id {$$=attrType($1,$3)};

IDs :  ( id // ',')+ {$$=$1} ;

