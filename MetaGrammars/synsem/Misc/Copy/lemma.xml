<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE mcgrammar SYSTEM "xmg-mctag.dtd,xml">

<mcgrammar>
    <lemmas>
        <lemma name="aimer"  cat="v" >
            <anchor tree_id="family[@name=n0Vn1]" >
                <filter>
                    <fs/>
                </filter>
                <sem>
                    <semclass name="binaryRel" >
                        <args>
                            <f name="theta1" >
                                <sym value="agent" />
                            </f>
                            <f name="rel" >
                                <sym value="aimer" />
                            </f>
                            <f name="theta2" >
                                <sym value="patient" />
                            </f>
                        </args>
                    </semclass>
                </sem>
            </anchor>
        </lemma>
        <lemma name="appeler"  cat="v" >
            <anchor tree_id="family[@name=n0Vn1]" >
                <filter>
                    <fs/>
                </filter>
                <sem>
                    <semclass name="binaryRel" >
                        <args>
                            <f name="theta1" >
                                <sym value="agent" />
                            </f>
                            <f name="rel" >
                                <sym value="appeler" />
                            </f>
                            <f name="theta2" >
                                <sym value="patient" />
                            </f>
                        </args>
                    </semclass>
                </sem>
            </anchor>
        </lemma>
        <lemma name="avoir"  cat="v" >
            <anchor tree_id="family[@name=Copule]" >
                <filter>
                    <fs/>
                </filter>
                <sem/>
            </anchor>
        </lemma>
        <lemma name="dormir"  cat="v" >
            <anchor tree_id="family[@name=n0V]" >
                <filter>
                    <fs/>
                </filter>
                <equation type="bot"  node_id="anc" >
                    <fs>
                        <f name="aux" >
                            <sym value="avoir" />
                        </f>
                    </fs>
                </equation>
                <equation type="bot"  node_id="anc" >
                    <fs>
                        <f name="aux-refl" >
                            <sym value="-" />
                        </f>
                    </fs>
                </equation>
                <sem>
                    <semclass name="unaryRel" >
                        <args>
                            <f name="theta1" >
                                <sym value="agent" />
                            </f>
                            <f name="rel" >
                                <sym value="dormir" />
                            </f>
                        </args>
                    </semclass>
                </sem>
            </anchor>
        </lemma>
        <lemma name="etre"  cat="v" >
            <anchor tree_id="family[@name=Copule]" >
                <filter>
                    <fs/>
                </filter>
                <sem/>
            </anchor>
        </lemma>
        <lemma name="il"  cat="cl" >
            <anchor tree_id="family[@name=Clitic]" >
                <filter>
                    <fs/>
                </filter>
                <sem>
                    <semclass name="basicProperty" >
                        <args>
                            <f name="rel" >
                                <sym value="il" />
                            </f>
                        </args>
                    </semclass>
                </sem>
            </anchor>
        </lemma>
        <lemma name="jean"  cat="n" >
            <anchor tree_id="family[@name=propername]" >
                <filter>
                    <fs/>
                </filter>
                <equation type="bot"  node_id="anc" >
                    <fs>
                        <f name="gen" >
                            <sym value="m" />
                        </f>
                    </fs>
                </equation>
                <equation type="bot"  node_id="anc" >
                    <fs>
                        <f name="det" >
                            <sym value="+" />
                        </f>
                    </fs>
                </equation>
                <sem>
                    <semclass name="basicProperty" >
                        <args>
                            <f name="rel" >
                                <sym value="jean" />
                            </f>
                        </args>
                    </semclass>
                </sem>
            </anchor>
        </lemma>
        <lemma name="marie"  cat="n" >
            <anchor tree_id="family[@name=propername]" >
                <filter>
                    <fs/>
                </filter>
                <equation type="bot"  node_id="anc" >
                    <fs>
                        <f name="gen" >
                            <sym value="f" />
                        </f>
                    </fs>
                </equation>
                <equation type="bot"  node_id="anc" >
                    <fs>
                        <f name="det" >
                            <sym value="+" />
                        </f>
                    </fs>
                </equation>
                <sem>
                    <semclass name="basicProperty" >
                        <args>
                            <f name="rel" >
                                <sym value="marie" />
                            </f>
                        </args>
                    </semclass>
                </sem>
            </anchor>
        </lemma>
        <lemma name="par"  cat="p" >
            <anchor tree_id="family[@name=void]" >
                <filter>
                    <fs/>
                </filter>
                <sem/>
            </anchor>
        </lemma>
        <lemma name="qui"  cat="c" >
            <anchor tree_id="family[@name=void]" >
                <filter>
                    <fs/>
                </filter>
                <sem/>
            </anchor>
        </lemma>
    </lemmas>
</mcgrammar>
